<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="SOALauncher.ConsoleTab"
	x:Name="UserControl"
	d:DesignWidth="820" d:DesignHeight="247" Unloaded="UserControl_Unloaded">

    <Grid x:Name="LayoutRoot">
        <ScrollViewer Name="ScrollView">
            <TextBox TextWrapping="Wrap" Name="ConsoleTextBox" Background="#7F000000" Foreground="White" FontFamily="Consolas" KeyDown="ConsoleTextBox_KeyDown" IsReadOnlyCaretVisible="True" />
        </ScrollViewer>
    </Grid>
</UserControl>